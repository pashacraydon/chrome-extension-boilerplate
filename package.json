{
  "name": "chrome_extension_boilerplate",
  "version": "0.0.1",
  "description": "A lean chrome extension boilerplate.",
  "scripts": {
    "tests": "mocha test/run.js",
    "build": "webpack --config webpack.config.js --progress --colors",
    "build:debug": "webpack --config webpack.config.js --progress --colors --display-error-details",
    "build:compress": "webpack --config webpack.config.js --progress --colors --display-error-details",
    "build:test": "webpack --config webpack.config.test.js --progress --colors --display-error-details",
    "watch": "webpack --config webpack.config.js --watch --colors",
    "watch:debug": "webpack --config webpack.config.js --watch --colors --display-error-details",
    "watch:test": "webpack --config webpack.config.test.js --watch --colors",
    "watch:test:debug": "webpack --config webpack.config.test.js --watch --colors --display-error-details"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pashasc/chrome_extension_boilerplate.git"
  },
  "author": "@pashasc",
  "bugs": {
    "url": "https://github.com/pashasc/chrome_extension_boilerplate/issues"
  },
  "homepage": "https://github.com/pashasc/chrome_extension_boilerplate",
  "dependencies": {
    "jquery": "^2.1.4",
    "lodash": "^4.13.1",
    "underscore-template-loader": "^0.7.3",
    "webpack": "^1.8.11",
    "chai": "^3.5.0"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.5.2",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.1.4",
    "babel-plugin-transform-react-jsx": "6.7.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "mocha": "^2.4.5",
    "sinon-chrome": "^1.1.2",
    "sinon": "^1.17.4",
    "jsdom": "^9.2.1",
    "wgxpath": "1.2.0"
  }
}
